@model OKEtask.Models.CountriesDetails
@{
    ViewBag.Title = "Home Page";
}
<div class="jumbotron">
    <div class="row">
        <div class="col-md-4 col-sm-6">
            <div class="form-group">
                @Html.LabelFor(m => m.CountryDetails.Id)
                @Html.TextBoxFor(m => m.CountryDetails.Id, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
        <div class="col-md-4 col-sm-6">
            <div class="form-group">
                @Html.LabelFor(m => m.CountryDetails.Name)
                @Html.TextBoxFor(m => m.CountryDetails.Name, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4 col-sm-6">
            <div class="form-group">
                @Html.LabelFor(m => m.CountryDetails.AreaCode)
                @Html.TextBoxFor(m => m.CountryDetails.AreaCode, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
        <div class="col-md-4 col-sm-6">
            <div class="form-group">
                @Html.LabelFor(m => m.CountryDetails.Capital)
                @Html.TextBoxFor(m => m.CountryDetails.Capital, new { @class = "form-control", @readonly = "readonly" })
            </div>
        </div>
    </div>

    @ViewBag.Message
</div>
